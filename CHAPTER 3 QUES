# WAP TO STORE SEVEN FRUITS IN A LIST ENTERED BY USER.
fruits=[]
f1=input("Enter fruit name:")
fruits.append(f1)
f2=input("Enter fruit name:")
fruits.append(f2)
f3=input("Enter fruit name:")
fruits.append(f3)
f4=input("Enter fruit name:")
fruits.append(f4)
f5=input("Enter fruit name:")
fruits.append(f5)
print(fruits)

# WAP TO ACCEPT THE MARKS OF 6 STUDENTS AND DISPLAY THEM IN A SORTED MANNER
student=[]
f1=input("Enter the marks of student")
student.append(f1)
f2=input("enter the marks of second student")
student.append(f2)
f3=input("enter the marks of third student")
student.append(f3)
f4=input("eneter the marks of fourth student")
student.append(f4)
f5=input("enter the marks of fifth student")
student.append(f5)
f6=input("eneter the marks of sixth student ")
student.append(f6)
student.sort
print(student)

